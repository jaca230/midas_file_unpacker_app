{
  "plugin_libraries": [
    "build/lib/libunpacker_data_products_nalu.so",
    "build/lib/libunpacker_stages_nalu.so"
  ],
  "pipeline": [
    {
      "id": "nalu_time_unpacker",
      "type": "ByteStreamToDataProductStage",
      "parameters": {
        "input_byte_stream_product_name": "bytestream_bank_AT%0",
        "root_class_name": "dataProducts::NaluTime",
        "data_product_name": "NaluTime",
        "field_mappings": {
          "collection_cycle_index":        { "offset": 0,  "size": 8, "endianness": "little", "type": "uint64_t" },
          "collection_cycle_timestamp_ns": { "offset": 8,  "size": 8, "endianness": "little", "type": "uint64_t" },
          "udp_time":                      { "offset": 16, "size": 8, "endianness": "little", "type": "double" },
          "parse_time":                    { "offset": 24, "size": 8, "endianness": "little", "type": "double" },
          "event_time":                    { "offset": 32, "size": 8, "endianness": "little", "type": "double" },
          "total_time":                    { "offset": 40, "size": 8, "endianness": "little", "type": "double" },
          "data_processed":                { "offset": 48, "size": 8, "endianness": "little", "type": "uint64_t" },
          "data_rate":                     { "offset": 56, "size": 8, "endianness": "little", "type": "double" }
        }
      },
      "next": []
    }
  ]
}
